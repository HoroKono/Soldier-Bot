import discord
from discord.ext.commands import Bot
from discord.ext import commands
import asyncio
import time
import random
import asyncio

Client = discord.Client()
client = commands.Bot(command_prefix = "^")

@client.event
async def on_ready():
  await client.change_presence(game=discord.Game(name='KILLING MAGGOTS!'))
  print("i am ready")
  print(client.user.id)
  print(client.user.name)
  print("maggot")


@client.event
#this is the message new members get when they join the server
async def on_member_join(member):
  print("a new robot has " + member.name + "joined")
  await client.send_message(member,"**Welcome To BFB&G read the rules**\r\n :scroll: commands :scroll:\r\n\r\n```^cloudran\r\n^flip\r\n^ask\r\n^tftube```\r\n\r\ni hope you enjoy your stay\r\n\r\n\r\n`(well is not like i can say anything else fuck my creator Gray Mann)`   ")
  print("send message to" + member.name)


